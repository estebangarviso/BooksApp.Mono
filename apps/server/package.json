{
	"$schema": "https://json.schemastore.org/package.json",
	"version": "0.0.1",
	"project": "@c2c/books",
	"name": "@c2c/books-server",
	"title": "Books Server",
	"description": "(((app-description)))",
	"scripts": {
		"start:dev": "env -e dev -m debug --log debug : vite-node --watch src/main.ts",
		"start:prod": "env -e prod -m debug : vite-node --watch src/main.ts",
		"build": "env -m build : vite build",
		"preview": "pnpm build & env -e dev -m build : node dist/main",
		"test:dev": "env -e dev -m test : vitest",
		"test:prod": "env -e prod -m test : vitest",
		"test:bench": "vitest bench",
		"test:mutation": "env -e dev -m test : stryker run",
		"env:schema:dev": "env schema -e dev -m build",
		"env:schema:prod": "env schema -e prod -m build",
		"lint": "eslint --cache --cache-location node_modules/.cache/eslint/",
		"format": "prettier --cache --write **/*.{ts,cts,mts,json,md,yml,yaml}",
		"clean": "pnpx del-cli dist .reports **/*.tsbuildinfo .rollup.cache .swc .node_modules"
	},
	"imports": {
		"#libs/ajv": "./src/libs/ajv/index.ts",
		"#libs/http": "./src/libs/http/index.ts",
		"#libs/decorators": "./src/libs/decorators/index.ts",
		"#testing": "./src/__tests__/index.ts",
		"#config": "./src/config/index.ts",
		"#db": "./src/database/default/index.ts",
		"#db/config": "./src/database/default/config/index.ts"
	},
	"dependencies": {
		"@fastify/static": "8.2.0",
		"@nestjs/common": "11.1.3",
		"@nestjs/core": "11.1.3",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-fastify": "11.1.3",
		"@nestjs/sequelize": "^11.0.0",
		"@nestjs/swagger": "11.2.0",
		"@sinclair/typebox": "^0.34.37",
		"ajv": "8.17.1",
		"ajv-formats": "3.0.1",
		"ajv-i18n": "4.2.0",
		"bcryptjs": "^3.0.2",
		"fastify": "5.4.0",
		"luxon": "3.6.1",
		"passport-jwt": "^4.0.1",
		"pg": "^8.16.3",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.2",
		"sequelize": "^6.37.7",
		"sequelize-typescript": "^2.1.6",
		"tslib": "2.8.1"
	},
	"devDependencies": {
		"@nestjs/testing": "11.1.3",
		"@stryker-mutator/core": "9.0.1",
		"@stryker-mutator/typescript-checker": "9.0.1",
		"@stryker-mutator/vitest-runner": "9.0.1",
		"@types/eslint": "9.6.1",
		"@types/luxon": "3.6.2",
		"@types/node": "22.16.2",
		"@types/passport-jwt": "^4.0.1",
		"@types/sequelize": "^4.28.20",
		"@v10e/env": "2.12.0",
		"@vitest/coverage-v8": "3.2.4",
		"eslint": "9.29.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-perfectionist": "4.15.0",
		"eslint-plugin-prettier": "5.5.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-regexp": "2.9.0",
		"eslint-plugin-sonarjs": "3.0.4",
		"eslint-plugin-unicorn": "59.0.1",
		"eslint-plugin-vitest": "0.5.4",
		"globals": "16.2.0",
		"prettier": "3.6.2",
		"terser": "5.43.1",
		"type-fest": "4.41.0",
		"typescript": "5.8.3",
		"typescript-eslint": "8.35.0",
		"unplugin-swc": "1.5.5",
		"vite": "7.0.0",
		"vite-node": "3.2.4",
		"vite-plugin-checker": "0.9.3",
		"vitest": "3.2.4",
		"vitest-mock-extended": "3.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@nestjs/core",
			"@scarf/scarf",
			"@swc/core",
			"esbuild"
		]
	},
	"type": "module",
	"engines": {
		"node": ">=22",
		"npm": ">=10",
		"pnpm": ">=10"
	},
	"license": "MIT",
	"keywords": [
		"nest",
		"fastify",
		"pnpm",
		"vite",
		"vitest",
		"typescript"
	]
}
